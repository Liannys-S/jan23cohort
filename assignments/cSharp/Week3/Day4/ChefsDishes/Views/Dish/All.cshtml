@{
    ViewData["Title"] = "All Dishes";
}
@model List<Dish>
<div class="container text-center mt-5">
    <h1 class="border-bottom border-dark mb-5">Yum! Check out our tasty dishes!</h1>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Chef</th>
                <th>Tastiness</th>
                <th>Calories</th>
            </tr>
        </thead>
        <tbody>
            @{
                foreach(Dish dish in Model){
                    <tr>
                        <td>@dish.Name</td>
                        <td>@dish.dishChef.FirstName @dish.dishChef.LastName</td>
                        <td>@dish.Tastiness</td>
                        <td>@dish.Calories</td>
                    </tr>
                }
            }
        </tbody>

    </table>
</div>
@{
}