@model Chef
<div class="container">
    
    <form asp-action="Add" asp-controller="Chef" method="Post">
        <div class="mb-3">
            <label class="form-label" asp-for="FirstName"></label>
            <input class="form-control" asp-for="FirstName">
            <span asp-validation-for="FirstName" class="text-danger"></span>
        </div>
        <div class="mb-3">
            <label class="form-label" asp-for="LastName"></label>
            <input class="form-control" asp-for="LastName">
            <span asp-validation-for="LastName" class="text-danger"></span>
        </div>
        <div class="mb-3">
            <label class="form-label" asp-for="Birthday"></label>
            <input class="form-control" asp-for="Birthday">
            <span asp-validation-for="Birthday" class="text-danger"></span>
        </div>
        <button class="btn btn-primary mt-3 float-end">Add Chef</button>
    </form>
</div>